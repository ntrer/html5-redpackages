.bottom-nav{
	width:100%;
	height: 40px;
	flex-direction: row; 
	display: flex;
	position: fixed;
	bottom: 0rem;
	top: auto;
	background: white;
}
.bottom-nav div{
	width: 33%;
}
/* .bottom-nav div */
.bottom-nav-imgbox{
	width: 30%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.bottom-nav-imgbox img{
	width: 100%;
	height: 100%;
}
.bottom-nav-home{
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column; 
	
}
.nav-item{
	display: flex;
	align-items: center;
	justify-content: center;
}
.nav-item img{
	width: 20px;
	height: 20px;
}
.extension-box{
	width: 30%;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column; 
}
.extension-box div{
	display: flex;
	align-items: center;
	justify-content: center;
}
.extension-box img{
	width: 20px;
	height: 20px;
}
.activity-bm-box{
	width: 40%;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}