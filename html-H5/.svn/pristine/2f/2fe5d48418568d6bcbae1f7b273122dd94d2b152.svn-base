  <script src="http://lib.sinaapp.com/js/jquery/1.9.1/jquery-1.9.1.min.js"></script>
  <script src="http://res.wx.qq.com/open/js/jweixin-1.4.0.js" type="text/javascript"></script>
  <script src="/js/h5Common.js"></script>
  <script>
    //本页加载默认分享设定
    //loadDefaultShareSettings();
  </script>
  <script>
    function brandList() {
      $.post(API_ROOT + "/new/client/brandmore/list/", {
        activityId: ACTIVITY_ID
      }, function (result) {
        if (result.ret != "200")
          alert(result.msg);
        else {
          var lis = "";
          for (var i = 0; i < result.data.length; i++) {
            lis +=
              '<div class="brand-item2" style="' + (i == result.data.length - 1 ? "" :
                " border-bottom: 1px solid #ccc;") + '">' +
              '<div style="display: flex; flex-direction: row; margin-top: 5px;align-items:center;">' + '<div>' +
              '<img src="' + API_ROOT + '/image/show/?token_id=' + TOKEN_ID + '&loginOs=0&imgType=&fileId=' + result
              .data[i].brandLogo + '" style="height: 110px; width: 110px;" />' + '</div>' +
              '<div style = "font-size: 10px; flex-grow: 1;display: flex; flex-direction: column; margin-left: 10px;" > ' +
              '<div style="font-size:18px;line-height:18px;font-weight:bold;">' + result.data[i].brandName +
              '</div>' +
              '<div class="brand-text-box text-hide3" style="margin-top:5px"> 简介：' + (result.data[i].introduce ||
                "") + '</div>' +
              '<div class="brand-text-box" style = "margin-top: 5px;height: 20px; line-height: 20px;overflow: hidden;"> ' +
              '联系方式: ' + (result.data[i].phone || "") + '</div> ' +
              '<div class="brand-text-box">地址：' + (result.data[i].address || "") + '</div > ' +
              '</div> ' +
              '<div style = " border-radius: 50%;  margin-right: 10px; "><a style="display:block;" href="tel:' +
              result.data[i].phone + '"><img style="width:40px;height:40px;" src="/img/h5/bohao.png"></a></div>' +
              '</div> ' +
              '</div> ';
          }
          $("[dom=brand-box]").html(lis);
        }
      });
    }
  </script>
  <script>
    $(function () {
      brandList();
    });
  </script>
</body>

