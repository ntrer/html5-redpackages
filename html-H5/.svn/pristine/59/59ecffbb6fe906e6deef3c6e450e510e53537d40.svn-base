		}, function (result) {
			if (result.ret == "202") {
				//报名为免费时不调用微信
				if (IS_ACTIVE_PROMPT_CODE == 1) {
					if (IS_LIVE == 1) {
						var isShowliveCode = nowInDateBetwen(LIVE_START_DATE, LIVE_END_DATE);
						if (isShowliveCode == true) {
