	position: absolute;
	margin-left: 15%;
	margin-top: 70%;
	background: -webkit-linear-gradient(left, rgba(255, 255, 255, 255), rgba(255, 0, 0, 0));
	/* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(right, rgba(255, 255, 255, 255), rgba(255, 0, 0, 0));
	/* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(right, rgba(255, 255, 255, 255), rgba(255, 0, 0, 0));
	/* Firefox 3.6 - 15 */
	background: linear-gradient(to right, rgba(255, 255, 255, 255), rgba(255, 0, 0, 0));
	/* 标准的语法（必须放在最后） */
}

