* {
	margin: 0;
	padding: 0;
	list-style: none;
}

.index-wrap {
	width: 100%;
	box-sizing: border-box;
}

.spike-top {
	display: flex;
	padding: 0.24rem;
	border-bottom: 1px solid #ededed;
}

.left-img {
	width: 2.5rem;
	height: 2.1rem;
	overflow: hidden;
	/* border: 1px solid #ededed; */
	border-radius: 0.1rem;
}

.left-img img {
	width: 100%;
	display: block;
	border-radius: 0.1rem;
	height: 100%;
}

.content-rig {
	margin-left: 0.24rem;
	width: 4.8rem;
}

.content-rig-title {
	font-size: 14px;
	font-weight: bold;
}

.price-num {
	display: flex;
	align-items: center;
	margin-top: 0.2rem;
}

.price-num li {
	font-size: 12px;
}

.price-num li>span {
	color: red;
}

.index-price {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 0.1rem;
}

.index-price>s {
	font-size: 10px;
	color: gray;
}

.index-price>p {
	color: red;
	font-size: 4px;
	font-weight: bold;
}

.index-price>p>span {
	font-size: 0.28rem;
	margin-left: 0.04rem;
}

.index-btn {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.index-btn-img {
	width: 1.5rem;
	height: 0.8rem;
	overflow: hidden;
	margin-top: 0.2rem;
}

.index-btn-img img {
	width: 100%;
	display: block;
	height: 100%;
}

.index-btn-red {
	font-size: 12px;
	background: red;
	color: #fff;
	border-radius: 0.1rem;
	text-align: center;
	padding: 0.2rem;
	width: 1rem;
	height: 0.2rem;
	line-height: 0.2rem;
	margin-top: 0.4rem;
}

.index-btn-gray {
	font-size: 12px;
	background: #FB4625;
	color: #fff;
	border-radius: 0.1rem;
	text-align: center;
	padding: 0.2rem;
	width: 1rem;
	height: 0.2rem;
	line-height: 0.2rem;
	margin-top: 0.4rem;

}

.index-label {
	height: 0.8rem;
	line-height: 0.8rem;
	background: rgb(211, 42, 42);
	color: #fff;
	text-align: center;
	font-size: 14px;
	margin-bottom: 0.2rem;
}

.banner-img {
	height: 3rem;
	overflow: hidden;
}

.banner-img img {
	width: 100%;
	display: block;
}

.main-img {
	height: 5rem;
	overflow: hidden;
	position: relative;
	border-top: 1px solid gainsboro;
	margin-top: 0.2rem;
	padding-top: 0.2rem;
	background: url('./../images/img1.jpg') no-repeat;
	background-size: 100% 100%;
}

.main-gradual {
	position: absolute;
	top: 1rem;
	right: 0.5rem;
	color: #fff;
	font-size: 12px;
	border-radius: 0.5rem;
	background: linear-gradient(135deg, red, orange);
	padding: 0.2rem 0.4rem;
}

.main-cont-img {
	height: 3rem;
	overflow: hidden;
	background: url('./../images/grouping.png') no-repeat;
	background-size: 100% 100%;
	margin-top: 2rem;
}

.main-cont-img>div {
	position: absolute;
	top: 3.4rem;
	right: 0.8rem;
	color: #fff;
	font-size: 12px;
	border-radius: 0.5rem;
	padding: 0.2rem;
	background: red;
}

.banner-share {
	height: 3rem;
	overflow: hidden;
	margin-top: 0.3rem;
}

.banner-share img {
	width: 100%;
	display: block;
}

.speed-wrap {
	margin: 0.24rem;
}

.speed-wrap-top {
	display: flex;
}

.speed-wrap-top div {
	font-size: 14px;
}

.speed-wrap-bot {
	width: 0.7rem;
	height: 0.7rem;
	margin-top: 0.3rem;
}

.speed-wrap-bot img {
	width: 100%;
	display: block;
	border-radius: 50%;
}

.speed-btn-red {
	height: 0.7rem;
	line-height: 0.7rem;
	background: red;
	border-radius: 0.05rem;
	text-align: center;
	font-size: 14px;
	color: #fff;
	margin: 0.3rem 0.2rem;
}

.partake-btn {
	height: 0.7rem;
	line-height: 0.7rem;
	border: 1px solid red;
	border-radius: 0.05rem;
	text-align: center;
	font-size: 14px;
	color: red;
	margin: 0.3rem 0.2rem;
	font-weight: bold;
}

.foot-gray {
	height: 0.5rem;
	background: rgb(51, 49, 49);
	font-size: 13px;
	color: #fff;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0.1rem 0;
}

.foot-gray div:nth-of-type(1) {
	width: 0.5rem;
	height: 0.5rem;
}

.foot-gray div:nth-of-type(1) img {
	width: 100%;
	display: block;
	border-radius: 50%;
}

.foot-gray div:nth-of-type(2) {
	margin-left: 0.2rem;
}

.covers {
	display: none;
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 9999999;
	background: rgba(0, 0, 0, 0.8);
}

.covers>div {
	display: table-cell;
	vertical-align: middle;
	text-align: center;
	width: 100%;
	height: 100%;
	color: white;
}

.covers>div input[type=button] {
	padding: 10px;
	background-color: #eee;
	border: 0;
	margin-top: 20px;
}

