		length: 20
	}, function (result) {
		orderLogs = result.data;
		if (orderLogs.length > 0) {
			barr(0);
		} else {
			$(".shadow-wrap").hide();
		}
	});

	function barr(index) {
		if (index + 1 > orderLogs.length) {
			index = 0;
		}
		$(".shadow-wrap").css("display", "flex");
